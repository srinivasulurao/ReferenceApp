                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Home Phone:"
                            android:textColor="#000000"
                            />
                        
                        <TextView
                            android:id="@+id/Fphone2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:text=""
                            android:textColor="@color/blue"
                            
                            android:textStyle="bold" />
                        
                    </LinearLayout>
                   

                    <TextView
                        android:id="@+id/aotd_delivery_PUaddress_txt_pup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:textColor="#000000" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="Instruction :"
                            android:textColor="#000000"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/aotd_delivery_PUInstruction_txt_pup"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:textColor="#000000" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:text="Delivery To :"
                        android:textColor="#000000"
                        android:textStyle="bold" />
                    
                    <LinearLayout 
                        android:id="@+id/lin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <TextView 
                            android:id="@+id/DL_Company"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Company"
                            android:textColor="#000000"
                            />
                        
                         <TextView 
                            android:id="@+id/DL_Address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Address"
                            android:textColor="#000000"
                            />
                          <TextView 
                            android:id="@+id/DL_Suite"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Suite"
                            android:textColor="#000000"
                            />
                         <TextView 
                            android:id="@+id/DL_City"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="City"
                            android:textColor="#000000"
                            />
                         <TextView 
                            android:id="@+id/DL_State"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="State"
                            android:textColor="#000000"
                            />
                         <TextView 
                            android:id="@+id/DL_Zip"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Zip"
                            android:textColor="#000000"
                            />
                    </LinearLayout>
                    
                    <LinearLayout 
                        android:id="@+id/lin_DL_CellPhone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView 
                            android:id="@+id/txtDLCellPhone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Cell Phone:"
                            android:textColor="#000000"
                            />
                        <TextView
                            android:id="@+id/phone1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:text=""
                            android:textColor="@color/blue"
                            android:textStyle="bold" />
                        
                    </LinearLayout>
                    
                    <LinearLayout 
                        android:id="@+id/lin_DL_HomePhone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView 
                            android:id="@+id/txtDLHomePhone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Home Phone:"
                            android:textColor="#000000"
                            />
                         <TextView
                            android:id="@+id/phone2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:text=""
                            android:textColor="@color/blue"
                            android:textStyle="bold" />
                        
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/options"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/showMap"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="Show Map"
                            android:textColor="@color/blue"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/aotd_delivery_DLaddress_txt_pup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:textColor="#000000" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="Instruction :"
                            android:textColor="#000000"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/aotd_delivery_DLInstruction_txt_pup"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:textColor="#000000" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/aotd_roundtrip_layout_pup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="Round Trip :"
                            android:textColor="#000000"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/aotd_roundtrip_pup"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:textColor="#000000" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:text="Order Details :"
                        android:textColor="#000000"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
