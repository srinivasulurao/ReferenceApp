                        <TextView 
                            android:id="@+id/txtDLCellPhone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Cell Phone:"
                            android:textColor="#000000"
                            />
                         <TextView
                            android:id="@+id/Fphone1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
