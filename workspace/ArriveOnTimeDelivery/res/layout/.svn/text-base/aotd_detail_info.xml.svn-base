<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:background="@drawable/aotd_background">
  
    <RelativeLayout
        android:layout_width="400dp"
        android:layout_height="fill_parent"
        android:layout_centerHorizontal="true"
       >
	
  
<TextView
    	android:layout_width="400dp" 
    	android:layout_height="wrap_content" 
    	android:text="Dispatch Screen"
    	android:gravity="center"
    	android:paddingBottom="8dp"
    	android:textColor="#4169E1"
    	android:textSize="25sp"
    	android:textStyle="bold"
    	android:background="@drawable/aotd_title"
    	android:id="@+id/detaildelivery_layout_title"
    	android:layout_centerHorizontal="true"/>



 <ScrollView 
		android:id="@+id/ScrollView01" 
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:scrollbars="none" 
  		android:layout_below="@+id/detaildelivery_layout_title">
<RelativeLayout
  android:layout_width="400dp"
  android:layout_height="fill_parent"
  android:layout_gravity="center_horizontal">

  <Button android:id="@+id/detail_btn_pickup"
  android:layout_width="wrap_content"
  android:text="pick-up"
  android:textColor="#000000"  
  android:layout_height="45dp"
  android:layout_alignParentLeft="true" 
  android:layout_marginTop="10dp"
  android:layout_marginLeft="10dp"/>
  
  <Button android:id="@+id/detail_btn_scan"
  android:layout_width="wrap_content"
  android:text="Scan"
  android:textColor="#000000"
  android:layout_height="45dp"
  android:layout_alignParentRight="true" 
  android:layout_marginTop="10dp"
  android:layout_marginRight="10dp"/>
  
  <TextView android:id="@+id/detail_txt_pickuptext"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="pickuptext"  
  android:clickable="true"   
  android:textStyle="bold"  
  android:textColor="#000000"                            
  android:layout_below="@+id/detail_btn_pickup"
  android:layout_marginTop="10dp" 
  android:layout_marginLeft="10dp"/>
  
  <TextView android:id="@+id/detail_txt_pickupinfo"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="fhdh"
  android:textColor="#000000"   
  android:layout_below="@id/detail_txt_pickuptext"                              
  android:layout_marginTop="10dp"
  android:layout_marginLeft="25dp"/>
  
  <TextView android:id="@+id/detail_txt_deliverytext"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content" 
  android:text="delivery"  
  android:textColor="#000000"  
  android:textStyle="bold"                              
  android:layout_below="@+id/detail_txt_pickupinfo"
  android:layout_marginTop="10dp" 
  android:layout_marginLeft="10dp"/>
  
  <TextView android:id="@+id/detail_txt_deliveryinfo"
  android:layout_width="wrap_content"
  android:textColor="#000000"  
  android:layout_height="wrap_content"
  android:text="fhdh"
  android:layout_below="@+id/detail_txt_deliverytext"                              
  android:layout_marginTop="10dp"
  android:layout_marginLeft="25dp" /> 
  
  <TextView android:id="@+id/detail_txt_wheretext"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="Where"  
  android:textColor="#000000"  
  android:clickable="true"   
  android:textStyle="bold"    
  android:layout_below="@+id/detail_txt_deliveryinfo"                          
  android:layout_marginTop="10dp" 
  android:layout_marginLeft="10dp" />
  
  <TextView android:id="@+id/detail_txt_whereinfo"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="fhdh"
  android:textColor="#000000"  
  android:layout_below="@+id/detail_txt_wheretext"                              
  android:layout_marginTop="10dp"
  android:layout_marginLeft="25dp" />
  
 <TextView android:id="@+id/detail_txt_whentext"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="When"  
  android:textColor="#000000"  
  android:clickable="true"   
  android:textStyle="bold"
  android:layout_below="@+id/detail_txt_whereinfo"                              
  android:layout_marginTop="10dp" 
  android:layout_marginLeft="10dp" />
  
  <TextView android:id="@+id/detail_txt_wheninfo"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="fhdh"
  android:textColor="#000000"  
  android:layout_below="@+id/detail_txt_whentext"                              
  android:layout_marginTop="10dp" 
  android:layout_marginLeft="25dp"/>
  
  <TextView android:id="@+id/detail_txt_whattext"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content" 
  android:text="What"  
  android:textColor="#000000"  
  android:clickable="true"   
  android:textStyle="bold" 
  android:layout_below="@+id/detail_txt_wheninfo"                              
  android:layout_marginTop="10dp"                              
  android:layout_marginLeft="10dp" /> 
  
  <TextView android:id="@+id/detail_txt_whatinfo"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content" 
  android:text="fhdh"
  android:textColor="#000000"  
  android:layout_below="@+id/detail_txt_whattext"                              
  android:layout_marginTop="10dp" 
  android:layout_marginLeft="25dp"
  />
  </RelativeLayout>
  </ScrollView>
  </RelativeLayout>
 </RelativeLayout>